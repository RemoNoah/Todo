{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    // Change the server, uid, pwd and database to your SQL Server
    "Default": "server=<URL>,<Port>;uid=<userName>;pwd=<yourPassword>;database=<dbName>;TrustServerCertificate=true"
  },
  "Jwt": {
    //Change the Issuer and Audience to your project name
    "Issuer": "Todo",
    "Audience": "Todo",
    //Add your new key
    "Key": "",
    //Set the ExpireMinutes to the time you want the token to expire
    "ExpireMinutes": 240
  },
  //TODO: Set the allowed Orgens (like https://localhost:7297)
  "AllowedOrigins": "https://localhost:*"
}
